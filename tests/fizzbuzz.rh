fizzbuzz := [n] -> (
  list := []
  i := 1
  i <= n |> (
    val := i {
      _ -/ 15 == 0 .. 'FizzBuzz'
      _ -/ 5 == 0 .. 'Buzz'
      _ -/ 3 == 0 .. 'Fizz'
      _
    }
    list := list ++ [val]
    i := i ++ 1
  )
  list
)
res := fizzbuzz(15)
res %= [1; 2; 'Fizz'; 4; 'Buzz'; 'Fizz'; 7; 8; 'Fizz'; 'Buzz'; 11; 'Fizz'; 13; 14; 'FizzBuzz']
