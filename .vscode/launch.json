    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        },
        {
            "name": "Launch rhumb test",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "cmd/rhi",
            "args": [
                "-test",
                "${workspaceFolder}/tests/repro_star_v2.rh",
            ],
            "env": {
                // "RHUMB_VISITOR_DEBUG": "1",
                // "RHUMB_CODE_ARRAY_DEBUG": "1",
                // "RHUMB_VM_DEBUG": "1"
            }
        },
        {
            "name": "Launch rhumb REPL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "console": "integratedTerminal",
            "program": "cmd/rhumb/main.go",
            "args": [
                "repl"
            ],
            "env": {
                "GOEXPERIMENT": "arenas",
                "RHUMB_VISITOR_DEBUG": "0",
                "RHUMB_CODE_ARRAY_DEBUG": "0",
                "RHUMB_VM_DEBUG": "0"
            }
        }

    ]
}