    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        },
        {
            "name": "Launch rhumb",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "cmd/rhumb/main.go",
            "args": [
                "line",
                "foo .= 10",
                "bar .= 3",
                "foo ** bar // 6"
            ]
        }

    ]
}